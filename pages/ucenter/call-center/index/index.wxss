/**index.wxss**/
.container {
    width: 650rpx;
    margin: 20rpx 50rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.menu-wrap {
    width: 100%;
    background: #ffffff;
    flex-direction: column;
    display: flex;
}
.menu-wrap-header{
    flex-direction: row;
    display: flex;
}
.menu-item-icon{
    width: 40rpx;
    height: 40rpx;
    margin: 30rpx 50rpx 0 20rpx;
    font-size: 35rpx;
    float: left;
}
.menu-item-right{
    width: 40rpx;
    height: 40rpx;
    margin: 30rpx 0rpx 0 50rpx;
    font-size: 35rpx;
    float: right;
}
.menu-item-text{
    width: 480rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 36rpx;
}
.menu-wrap-list{
    width: 530rpx;
    margin-left: 110rpx;
    border-bottom:1rpx solid #eeeeee;
}